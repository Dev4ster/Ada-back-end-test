{
  "name": "desafio-back-end",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Victor Menezes <victormenezes35@hotmail.comm>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && yarn babel src --extensions \".js,.ts\" --out-dir dist --copy-files --ignore \"**/*.spec.ts\" --ignore \"**/*.test.ts\" --no-copy-ignored",
    "start": "node dist/main/server.js",
    "start:debug": "nodemon --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "dev": "yarn nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src/main/server.ts",
    "dev:stop": "yarn dev:container:stop",
    "dev:container": "yarn build && docker-compose --env-file .env up -d",
    "dev:container:stop": "docker-compose down",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "yarn test --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:coverage": "yarn test --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.13",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^28.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.11",
    "supertest": "^6.2.4",
    "uuid": "^8.3.2"
  }
}
